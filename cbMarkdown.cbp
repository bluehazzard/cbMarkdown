<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cbMarkdown" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="cb_plugin_wx30">
				<Option output="output/wx30/cbMarkdown" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/default" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="$(#cb)/devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx30.include)" />
					<Add directory="$(#wx30.lib)/gcc_dll/mswu" />
				</Compiler>
				<Linker>
					<Add library="wxmsw30u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx30.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="cmd /c if not exist plugin_wx30 mkdir plugin_wx30" />
					<Add after="zip -j9 plugin_wx30\cbMarkdown.zip manifest.xml" />
					<Add after="zip -9  plugin_wx30\cbMarkdown.zip images\*" />
					<Add after="zip -j9 plugin_wx30\cbMarkdown.zip res\*" />
					<Add after="zip -j9 plugin_wx30\cbMarkdown.cbplugin $(TARGET_OUTPUT_FILE) plugin_wx30\cbMarkdown.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="to_codeblocks_wx30">
				<Option output="$(#cb)/devel30/share/codeblocks/plugins/cbMarkdown" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs/to_codeblocks_wx30" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-splash-screen --verbose -p debug --no-dde --no-check-associations " />
				<Option host_application="$(#cb)/devel30/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add directory="$(#wx30.include)" />
					<Add directory="$(#wx30.lib)/gcc_dll/mswu" />
				</Compiler>
				<Linker>
					<Add library="wxmsw30u" />
					<Add library="codeblocks" />
					<Add directory="$(#wx30.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)\devel30/share/codeblocks/cbMarkdown.zip manifest.xml" />
					<Add after="zip -j9 $(#cb)\devel30/share/codeblocks/cbMarkdown.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="cb_plugin_wx30;to_codeblocks_wx30;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-std=gnu++11" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#cb)/include" />
			<Add directory="$(#cb)/sdk/wxscintilla/include" />
			<Add directory="$(#cb)/include/tinyxml" />
			<Add directory="$(#discount.INCLUDE)" />
		</Compiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="libmarkdown.a" />
			<Add directory="$(#discount.LIB)" />
		</Linker>
		<Unit filename="MarkdownConfigPanel.cpp" />
		<Unit filename="MarkdownConfigPanel.h" />
		<Unit filename="cbMarkdown.cpp" />
		<Unit filename="cbMarkdown.h" />
		<Unit filename="cbmarkdownview.cpp" />
		<Unit filename="cbmarkdownview.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="textProvider.cpp" />
		<Unit filename="textProvider.h" />
		<Unit filename="wxsmith/MarkdownConfigPanel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/MarkdownConfigPanel.wxs" src="MarkdownConfigPanel.cpp" hdr="MarkdownConfigPanel.h" fwddecl="0" i18n="1" name="MarkdownConfigPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
